Category}\r\n                    items={categoryNames}\r\n                />\r\n                <SortPopup\r\n                    items={sortItems}\r\n                    activeSortType={sortBy.type}\r\n                    onClickSort={onSelectSortType}\r\n                />\r\n            </div>\r\n            <h2 className=\"content__title\">Все пиццы</h2>\r\n            <div className=\"content__items\">\r\n                { isLoaded\r\n                    ? items.map( obj => <PizzaBlock\r\n                        onClickAddPizza={handleAddPizza}\r\n                        key={`pizza_${obj.id}`}\r\n                        addedCount = {cartItems[obj.id] && cartItems[obj.id].length}\r\n                        {...obj}\r\n                    />)\r\n                    : [...Array(12)].map((item, index) => <PizzaLoadingBlock key={`loading_${index}`} />)}\r\n            </div>\r\n        </div>\r\n    );\r\n};\r\n\r\n\r\nexport default Home;","D:\\ReactJs\\react-food\\src\\components\\Button.jsx",[],"D:\\ReactJs\\react-food\\src\\redux\\actions\\cart.js",[],"D:\\ReactJs\\react-food\\src\\redux\\reducers\\cart.js",["71","72"],"D:\\ReactJs\\react-food\\src\\index.js",[],["73","74"],"D:\\ReactJs\\react-food\\src\\redux\\reducers\\pizzas.js",[],"D:\\ReactJs\\react-food\\src\\pages\\index.js",[],["75","76"],"D:\\ReactJs\\react-food\\src\\redux\\actions\\pizzas.js",[],"D:\\ReactJs\\react-food\\src\\pages\\Cart.jsx",[],"D:\\ReactJs\\react-food\\src\\components\\SortPopup.jsx",[],"D:\\ReactJs\\react-food\\src\\components\\PizzaBlock\\LoadingBlock.jsx",[],"D:\\ReactJs\\react-food\\src\\redux\\actions\\filters.js",[],"D:\\ReactJs\\react-food\\src\\components\\CartItem.jsx",[],"D:\\ReactJs\\react-food\\src\\components\\index.js",["77"],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":33,"column":8,"nodeType":"84","endLine":33,"endColumn":26,"suggestions":"85"},{"ruleId":"82","severity":1,"message":"86","line"